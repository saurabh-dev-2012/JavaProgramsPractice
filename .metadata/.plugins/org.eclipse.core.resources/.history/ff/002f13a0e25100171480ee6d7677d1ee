package com.mypractice.array;

public class ArrayPractice {
	
	public static void sortArray(int[] array){//1,5,5,2,2,7
//		int 
		int n=array.length;
		for(int i=0;i<n;i++){
			for(int j=i+1;j<n;j++){
				if(array[i] > array[j] ){
					int temp = array[i];
					array[i] = array[j];
					array[j] = temp;
				}
			}
		}
//		int temp=0;
//		for(int i=0;)
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int[] array={1,5,2,6,4,7};
        ArrayPractice.sortArray(array);
	}

}
